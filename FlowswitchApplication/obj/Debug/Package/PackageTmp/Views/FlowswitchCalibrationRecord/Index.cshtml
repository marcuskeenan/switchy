@model IEnumerable<FlowswitchApplication.Models.CalibrationRecord>

@{
    ViewBag.Title = "Index";
}
@Html.Partial("NavBarPartial")
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaxInches)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxGallons)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TripInches)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TripGallons)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateAdded)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Note)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Flowswitch.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceRequest.FamisWorkOrder)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Technician.LastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaxInches)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxGallons)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TripInches)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TripGallons)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateAdded)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Note)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Flowswitch.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceRequest.FamisWorkOrder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Technician.LastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
