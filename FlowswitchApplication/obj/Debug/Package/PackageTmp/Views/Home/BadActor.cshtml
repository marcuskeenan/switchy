@model IEnumerable<FlowswitchApplication.ViewModels.CalibrationFlowswitchGroup>

@{
    ViewBag.Title = "Bad Actor Statistics";
}
@Html.Partial("NavBarPartial")

<h2>Flow Switch Bad Actor Statistics</h2>

 
    @foreach (var item in Model)
    {
        
            <div class="col-lg-2">
                
                <div class="list-group">
                    <a href="#" class="list-group-item active">
                        <h4 class="list-group-item-heading">@Html.DisplayFor(modelItem => item.Row)</h4>
                        <h4 class="list-group-item-heading">@Html.DisplayFor(modelItem => item.Name)</h4>
                        <p class="list-group-item-text"><h4>@item.CalibrationCount</h4></p>
                    </a>
                </div>
            
          </div>
       
    }
